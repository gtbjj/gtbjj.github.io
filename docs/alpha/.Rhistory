library("dplyr", "Hmisc", "corrplot")
library("Hmisc")
library("corrplot")
raw_data <- read.delim("test-data.tsv")
df <- as.matrix(as.data.frame(lapply(raw_data, as.numeric)))
summary(raw_data)
rcorr9df, type=c("pearson"))
rcorr(df, type=c("pearson"))
clear
res <- rcorr(df, type=c("pearson"))
png(filename=(test-corr.png", height=600, width=600, bg="white")
png(filename="test-corr.png", height=600, width=600, bg="white")
corrplot(res$r, method="number", p.mat = res$P, sig.level = 0.05, insig = blank)
corrplot(res$r, method="number", p.mat = res$P, sig.level = 0.05, insig = "blank")
sink("test-corr.png")
corrplot(res$r, method="number", p.mat = res$P, sig.level = 0.05, insig = "blank")
png("test-corr.png")
corrplot(res$r, method="number", p.mat = res$P, sig.level = 0.05, insig = "blank")
dev.off()
quit()
